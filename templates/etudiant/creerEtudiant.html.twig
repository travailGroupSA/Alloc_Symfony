{% extends 'base.html.twig' %}

{% block title %}creation etudiant{% endblock %}

{% block nav %}
{% endblock %}
{% block changer %}
<div class="overflow-auto card bg-dark hv-100 col-11" style=" height:  90vh;">
  <img  class="card-img-top">
  <div class="card-body">
    {{form_start(form)}}
    <div class="row">
        <div class="col">
            {{ form_row(form.prenom) }}
        </div>
        <div class="col">
            {{ form_row(form.nom) }}
        </div>
    </div>
    <div class="row">
      <div class="col">
        {{ form_row(form.email) }}
      </div>
      <div class="col">
        {{ form_row(form.telephone) }}
      </div>
    </div>
    <div class="row">
      <div class="col">
        {{ form_row(form.birthday) }}
      </div>
      <div class="col">
        {{ form_row(form.boursier) }}
      </div>
    </div>
    <div class="row">
      <div class="col">
        {{ form_row(form.chambre) }}
      </div>
      <div class="col">
        {{ form_row(form.adresse) }}
      </div>
    </div>
    <input type="submit" class="btn btn-primary" value="Enregistrer">
    <a href="{{path('etudiant')}}"><input type="button" class="btn btn-warning float-right" value="Annuler"></a>
    {{form_end(form)}}
</div>
{% endblock %}
{% block foot %}{% endblock %}


